<exercise id="fs-id2777936">
<problem id="fs-id1390811">
<para id="fs-id1390813"><m:math><m:mrow><m:mn>493</m:mn><m:mo>−</m:mo><m:mn>370</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1693352">
<para id="fs-id1693354">123</para>
</solution>
</exercise>
<exercise id="fs-id1732685">
<problem id="fs-id1732687">
<para id="fs-id2368077"><m:math><m:mrow><m:mn>5,946</m:mn><m:mo>−</m:mo><m:mn>4,625</m:mn></m:mrow></m:math></para>
</problem>
