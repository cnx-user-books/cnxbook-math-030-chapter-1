</solution>
</exercise>
<exercise id="fs-id1160640">
<problem id="fs-id1563482">
<para id="fs-id1563484"><m:math><m:mrow><m:mn>75</m:mn><m:mo>−</m:mo><m:mn>47</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id2587507">
<para id="fs-id2587509">28</para>
</solution>
</exercise>
<exercise id="fs-id1511785">
<problem id="fs-id1511787">
<para id="fs-id2495676"><m:math><m:mrow><m:mn>461</m:mn><m:mo>−</m:mo><m:mn>239</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id2788989">
<para id="fs-id2645573">222</para>
</solution>
</exercise>
<exercise id="fs-id1269410">
<problem id="fs-id2662315">
<para id="fs-id2662317"><m:math><m:mrow><m:mn>525</m:mn><m:mo>−</m:mo><m:mn>179</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1360117">
<para id="fs-id1360119">346</para>
</solution>
</exercise>
<exercise id="fs-id1185192">
<problem id="fs-id1185194">
<para id="fs-id1263091"><m:math><m:mrow><m:mn>6,318</m:mn><m:mo>−</m:mo><m:mn>2,799</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1774863">
<para id="fs-id1525134">3,519</para>
</solution>
</exercise>
