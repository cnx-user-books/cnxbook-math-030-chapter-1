<row valign="top">
<entry valign="top" align="left"><m:math><m:mrow><m:mtext>Multiplication</m:mtext></m:mrow></m:math></entry>
<entry valign="top" align="left"><m:math><m:mrow><m:mo>×</m:mo></m:mrow></m:math><newline/><m:math><m:mrow><m:mo>·</m:mo></m:mrow></m:math><newline/><m:math><m:mrow><m:mo>(</m:mo><m:mspace width="0.2em"/><m:mo>)</m:mo></m:mrow></m:math></entry>
<entry valign="top" align="left"><m:math><m:mrow><m:mn>3</m:mn><m:mspace width="0.2em"/><m:mo>×</m:mo><m:mspace width="0.2em"/><m:mn>8</m:mn></m:mrow></m:math><newline/><m:math><m:mrow><m:mn>3</m:mn><m:mo>·</m:mo><m:mn>8</m:mn></m:mrow></m:math><newline/><m:math><m:mrow><m:mn>3</m:mn><m:mo stretchy="false">(</m:mo><m:mn>8</m:mn><m:mo stretchy="false">)</m:mo></m:mrow></m:math></entry>
<entry valign="top" align="left"><m:math><m:mrow><m:mtext>three times eight</m:mtext></m:mrow></m:math></entry>
